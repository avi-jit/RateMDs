          type="password"
          id="login-password"
          name="login-password"
          ng-model="loginFormData.password"
          placeholder="Password"
          class="form-control"
          required
          rmd-form-field
        >
      </div>
    </div>

    <div class="modal-auth-forgot">
      <a href="/accounts/forgot_password">Forgot password?</a>
    </div>

