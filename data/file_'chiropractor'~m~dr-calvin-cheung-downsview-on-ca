  		onSuccess : loginFormSuccess
  	}"
  >
    <div class="alert alert-danger" ng-show="loginError">
      [[ loginError ]] 
    </div>
    
     
    <div class="form-group">
      <label for="login-email" class="hidden">Email</label>
      <div class="input-group">
        <span class="input-group-addon"><i class="fa fa-envelope"></i></span>
        <input
          type="email"
          id="login-email"
          name="email"
          ng-model="loginFormData.email"
          placeholder="Email"
          class="form-control"
          required
          rmd-form-field="{
          	validateInline:false
          }"
        >
      </div>
    </div>
    <div class="form-group">
      <label for="login-password" class="hidden">Password</label>
      <div class="input-group">
