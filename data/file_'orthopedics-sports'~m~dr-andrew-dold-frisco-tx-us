          type="email"
          id="login-email"
          name="email"
          ng-model="loginFormData.email"
          placeholder="Email"
          class="form-control"
          required
          rmd-form-field="{
          	validateInline:false
          }"
        >
      </div>
    </div>
    <div class="form-group">
      <label for="login-password" class="hidden">Password</label>
