<script type="text/ng-template" id="auth/signup.jinja">
  
<div class="modal-body modal-auth">
  <h4
    class="modal-title"
    ng-class="{
      'model-title-message': options.doctor || options.rating || options.follow || options.message
    }"
  >Signup to RateMDs</h4>

  <form
    name="signupForm"
    novalidate
    rmd-form="{
      submitUrl: 'api:signup',
      onSuccess: signupFormSuccess
    }"
  >

    <p ng-show="options.doctor" class="modal-auth-message">
      <strong>Doctors:</strong>
      The first step in claiming your profile is signing up for an account.
    </p>

    <p ng-show="options.rating" class="modal-auth-message">
      Signup to edit this rating in the future, and keep
      track of doctor responses.
    </p>

    <p ng-show="options.follow" class="modal-auth-message">
      Signup to follow this doctor and keep track of their
      latest ratings and updates.
    </p>

    <p ng-show="options.message" class="modal-auth-message">
      [[ options.message ]]
    </p>

    <div class="form-group">
      <label for="email" class="control-label hidden">Email</label>
      <div class="input-group">
        <span class="input-group-addon"><i class="fa fa-envelope"></i></span>
        <input
          type="email"
          id="email"
          ng-model="signupFormData.email"
          placeholder="Email"
          name="email"
          class="form-control"
          required
          rmd-form-field="{
          	validateInline:false
          }"
        >
      </div>
    </div>

    <div class="form-group">
      <label for="password" class="control-label hidden">Password</label>
      <div class="input-group">
        <span class="input-group-addon"><i class="fa fa-asterisk"></i></span>
        <input
          type="password"
          id="password"
          name="password"
          ng-model="signupFormData.password"
          placeholder="Password"
          class="form-control"
          required
          rmd-form-field="{
          	validateInline:false
          }"
        >
      </div>
    </div>

    <div class="form-group">
      <label for="confirm-password" class="control-label hidden">Confirm Password</label>
      <div class="input-group">
        <span class="input-group-addon"><i class="fa fa-asterisk"></i></span>
        <input
          type="password"
          id="confirm-password"
          name="confirm-password"
          placeholder="Confirm Password"
          class="form-control"
          ng-model="confirmPassword"
          ui-validate="'$value==signupFormData.password'"
          ui-validate-watch="'signupFormData.password'"
          required
          rmd-form-field="{
          	validator: 'Your passwords do not match',
          	validateInline:false
          }"
        >
        </div>
    </div>

    <div class="form-group modal-auth-opin modal-auth-opin-first">
      <label>
        <input
          type="checkbox"
          name="marketing_okay"
          ng-model="signupFormData.marketing_okay"
          rmd-form-field
        >
        Send me updates via email
      </label>
    </div>

    <div class="form-group modal-auth-opin">
      <label>
        <input
          type="checkbox"
          name="tos"
          ng-model="signupFormData.tos_okay"
          rmd-form-field
          required
        >
        I agree to <a href="/about/terms/">RateMDs terms of use</a>
      </label>
    </div>

    <button type="submit" class="btn btn-success btn-block btn-lg">Signup</button>
    <a href="javascript:;" ng-click="$dismiss();showLogin()">Login</a> or
    <a href="javascript:;" ng-click="$dismiss()">Cancel</a>

  </form>

</div>
</script>
<script type="text/ng-template" id="dropdown.jinja">
  
<div class="btn-group dropdown">
  <a
    class="btn btn-default dropdown-toggle"
    data-toggle="dropdown"
    ng-class="{disabled: disabled}"
  >
    <span ng-show="value==null">Choose a [[ name ]]</span>
    <span ng-show="value!=null">
      <span ng-show="!!name">[[ name ]]:</span>
      [[ valueName ]]
    </span>
    <i class="fa fa-caret-down"></i>
  </a>
  <ul class="dropdown-menu" role="menu">
    <li ng-repeat="option in options">
      <a href="javascript:;" ng-click="setValue(option[0])">[[ option[1] ]]</a>
    </li>
  </ul>
</div>
</script>
<script type="text/ng-template" id="filter_dropdown.jinja">
  
<div class="btn-group filter-dropdown" ng-class="{open: isOpen}">
  <a
    class="btn btn-default dropdown-toggle"
    ng-click="isOpen = !isOpen"
    ng-class="{disabled: disabled}"
  >
    <span ng-show="value==null">Choose a [[ name ]]</span>
    <span ng-show="value!=null">[[ name ]]: [[ valueName ]]</span>
    <i class="fa fa-caret-down"></i>
  </a>
  <ul class="dropdown-menu" role="menu" ng-show="isOpen">
    <div class="form-group">
      <input
        type="text"
        ng-model="filter"
        class="form-control"
        placeholder="Find a City"
      >
    </div>
    <li ng-repeat="option in filteredOptions">
      <a ng-show="!goto" href="javascript:;" ng-click="setValue(option[0])">[[ option[1] ]]</a>
      <a ng-show="goto" ng-href="[[ option[0] ]]">[[ option[1] ]]</a>
    </li>
  </ul>
</div>
</script>
<script type="text/ng-template" id="star_rating.jinja">
  
<span class="star-rating" ng-class="{'star-rating-empty': count == 0}">
  <span class="stars"><span class="star"
    ng-class="{
        half: rating >= 4.5 && rating < 5,
        selected: rating >= 5,
    }"
    ></span><span class="star"
      ng-class="{
        half: rating >= 3.5 && rating < 4,
        selected: rating >= 4,
      }"
    ></span><span class="star"
      ng-class="{
        half: rating >= 2.5 && rating < 3,
        selected: rating >= 3,
      }"
    ></span><span class="star"
      ng-class="{
        half: rating >= 1.5 && rating < 2,
        selected: rating >= 2,
      }"
    ></span><span class="star"
      ng-class="{
        half: rating >= 0.5 && rating < 1,
        selected: rating >= 1,
      }"
    ></span>
  </span>
</span>
<div class="star-rating-count" ng-show="count">
  [[ count ]] reviews
</div>
</script>
<script type="text/ng-template" id="star_rating_input.jinja">
  
<span class="star-rating star-rating-input star-rating-empty">

  <span class="stars"><span
      class="star star-5"
      ng-click="setValue(5)"
      data-value="5"
      ng-class="{selected: selected==5}"
    ></span><span
      class="star star-4"
      ng-click="setValue(4)"
      data-value="4"
      ng-class="{selected: selected==4}"
    ></span><span
      class="star star-3"
      ng-click="setValue(3)"
      data-value="3"
      ng-class="{selected: selected==3}"
    ></span><span
      class="star star-2"
      ng-click="setValue(2)"
      data-value="2"
      ng-class="{selected: selected==2}"
    ></span><span
      class="star star-1"
      ng-click="setValue(1)"
      data-value="1"
      ng-class="{selected: selected==1}"
    ></span></span>

  <span class="hint" ng-show="(selected==5 && hover==null) || hover==5">Excellent</span>
  <span class="hint" ng-show="(selected==4 && hover==null) || hover==4">Good</span>
  <span class="hint" ng-show="(selected==3 && hover==null) || hover==3">Okay</span>
  <span class="hint" ng-show="(selected==2 && hover==null) || hover==2">Poor</span>
  <span class="hint" ng-show="(selected==1 && hover==null) || hover==1">Terrible</span>

