          type="password"
          id="login-password"
          name="login-password"
          ng-model="loginFormData.password"
          placeholder="Password"
          class="form-control"
          required
          rmd-form-field
        >
      </div>
    </div>

    <div class="modal-auth-forgot">
      <a href="/accounts/forgot_password">Forgot password?</a>
    </div>

    <button class="btn btn-success btn-block btn-lg" >Login</button>
    <a href="javascript:;" ng-click="$dismiss();showSignup()">Signup</a> or
    <a href="javascript:;" ng-click="$dismiss()">Cancel</a>

